{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "test": "env-cmd -f .env-test ts-node -r tsconfig-paths/register node_modules/.bin/mocha \"./src/{,!(node_modules)/**}/*.test.ts\" --require source-map-support/register --recursive --exit",
    "dev": "nodemon src/server.ts",
    "lint": "eslint .",
    "build": "npm run build-ts && npm run migrate",
    "build-ts": "tsc && cpy \"src/**/*.yaml\" dist && cpy \"src/**/*.md\" dist",
    "migrate": "ts-node -r tsconfig-paths/register ./node_modules/.bin/sequelize db:migrate",
    "migrate:test": "env-cmd -f .env-test sequelize db:migrate",
    "migrate:all": "npm run migrate && npm run migrate:test"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "body-parser": "^2.2.0",
    "compression": "^1.8.0",
    "express": "^4.18.2",
    "express-bearer-token": "^3.0.0",
    "express-jwt": "^8.5.1",
    "morgan": "^1.10.0",
    "pg": "^8.15.6",
    "redoc-express": "^2.1.0",
    "sequelize": "^6.37.7",
    "sequelize-cli": "^6.6.2",
    "source-map-support": "^0.5.21",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.1",
    "@types/body-parser": "^1.19.5",
    "@types/chai": "^5.2.1",
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.21",
    "@types/mocha": "^10.0.10",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.15.3",
    "@types/sequelize": "^4.28.20",
    "@types/supertest": "^6.0.3",
    "chai": "^5.2.0",
    "concurrently": "^9.1.2",
    "cpy-cli": "^5.0.0",
    "env-cmd": "^10.1.0",
    "eslint": "^9.25.1",
    "mocha": "^11.1.0",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript-eslint": "^8.31.1"
  }
}
